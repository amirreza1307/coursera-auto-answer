{
  "manifest_version": 3,
  "name": "coursera auto answer",
  "version": "1.0",
  "description": "a google extension to answer coursera quiz with chatgpt",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "service-worker.js"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_title": "Action Button"
  }
}